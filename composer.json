{
    "name": "mehmetbulut/zoopla",
    "description": "PHP library for the Zoopla Real Time Property Datafeed Zoopla. https://realtime-listings.webservices.zpg.co.uk/docs/latest/documentation.html",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mehmetbltt",
            "email": "mehmetbltt@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*",
        "myclabs/php-enum": "1.6.6",
        "guzzlehttp/guzzle": "6.5.5",
        "justinrainbow/json-schema": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.27"
    },
    "autoload": {
        "psr-4": {
            "mehmetbulut\\Zoopla\\": "src/",
            "mehmetbulut\\Zoopla\\Tests\\": "tests"
        }
    }
}
